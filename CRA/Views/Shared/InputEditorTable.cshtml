@model CRA.Models.Shared.ItemModel
<p><span class="glyphicon glyphicon-plus green pointer" data-bind="click:@(Model.GetParentDataBoundUniqueClientCode()).create@(Model.ClientCode)"></span></p>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th class="id-column">

            </th>
            @foreach (var label in Model.LabelList)
            {
                <th>@(label)</th>
            }
        </tr>
    </thead>
    <tbody data-bind="foreach:@Model.GetDataBoundUniqueClientCode()">
        <tr>
            <td><span class="glyphicon glyphicon-remove delete-action pointer" data-bind="click:$root.@(Model.GetParentDataBoundUniqueClientCode()).deletemodalitem@(Model.ClientCode)"></span></td>
            @foreach (var code in Model.CodeList)
            {
                <td data-bind="text:@(code)"></td>
            }
        </tr>
    </tbody>
</table>




